<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/light"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bloom.pes_sensors.MainActivity">

    <TextView
        android:id="@+id/intensity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="76dp" />

    <TextView
        android:id="@+id/pressure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="141dp" />

    <TextView
        android:id="@+id/temperature"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="212dp" />

    <Button
        android:id="@+id/button_intensity"
        android:layout_width="212dp"
        android:layout_height="wrap_content"
        android:text="Light Intensity"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="53dp" />

    <Button
        android:id="@+id/button_pressure"
        android:layout_width="212dp"
        android:layout_height="wrap_content"
        android:text="Pressure"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="126dp" />

    <Button
        android:id="@+id/button_temp"
        android:layout_width="212dp"
        android:layout_height="wrap_content"
        android:text="Temperature"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="196dp" />

    <Button
        android:id="@+id/button_accel"
        android:layout_width="212dp"
        android:layout_height="wrap_content"
        android:text="Accelerometer"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="267dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="X Axis"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="348dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Y Axis"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="348dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Z Axis"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="348dp" />

    <TextView
        android:id="@+id/accel_x"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="374dp" />

    <TextView
        android:id="@+id/accel_y"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="374dp" />

    <TextView
        android:id="@+id/accel_z"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="374dp" />

</android.support.constraint.ConstraintLayout>
